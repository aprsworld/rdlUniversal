.................... 	spi_write2(0x00); 
.................... 	spi_write2(0x00); 
.................... 	spi_write2(0x00); 
....................  
.................... 	fprintf(stream_wireless,"# Dataflash page read from %lu\r\n",page>>2); 
.................... 	/* start reading */ 
.................... 	for ( l=0 ; l<DATAFLASH_BYTES_PER_PAGE ; l++ ) { 
